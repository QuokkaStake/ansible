[Unit]
Description=proxmox-interacter
After=network-online.target

[Service]
User={{ ansible_user }}
TimeoutStartSec=0
CPUWeight=95
IOWeight=95
ExecStart={{ ansible_user_dir }}/go/bin/proxmox-interacter --config {{ ansible_user_dir }}/config/proxmox-interacter.toml
Restart=always
RestartSec=2
LimitNOFILE=800000
KillSignal=SIGTERM

[Install]
WantedBy=multi-user.target
