[Unit]
Description=grafana-interacter
After=network-online.target

[Service]
User={{ ansible_user }}
TimeoutStartSec=0
CPUWeight=95
IOWeight=95
ExecStart={{ ansible_user_dir }}/go/bin/grafana-interacter --config {{ ansible_user_dir }}/config/grafana-interacter.yml
Restart=always
RestartSec=2
LimitNOFILE=800000
KillSignal=SIGTERM

[Install]
WantedBy=multi-user.target
